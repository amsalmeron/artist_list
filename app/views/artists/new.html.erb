<%= form_with url: "/artists", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :birth_year %>
  <%= form.text_field :birth_year %>
  <%= form.label :living, "Living (true/false)" %>
  <%= form.text_field :living %>

  <%= form.submit "Create Artist" %>
<% end %>
