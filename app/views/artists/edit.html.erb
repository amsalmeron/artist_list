<%= form_with url: "/artists/#{@artist.id}", method: :patch, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :birth_year %>
  <%= form.text_field :birth_year %>
  <%= form.label :living %>
  <%= form.text_field :living %>
  <%= form.submit 'Update Artist' %>
<% end %>
